{
    "name": "dedipanel/phpseclib-wrapper-bundle",
    "description": "PHPSeclib Wrapper with connections manager, logging, os-specific helpers, ...", 
    "keywords": [ "phpseclib", "ssh", "sftp", "shell" ], 
    "license": "MIT", 
    "authors": [
        {
            "name": "Albin Kerouanton", 
            "role": "Developer"
        }
    ], 
    "require": {
        "php": ">=5.3.2", 
        "symfony/framework-bundle": ">=2.1",
        "phpseclib/phpseclib": "dev-master", 
        "psr/log": ">=1.0,<2.0"
    },
    "require-dev": {
		"phpunit/phpunit": "3.7.x-dev"
    },
    "autoload": {
        "psr-0": { "DP\\PHPSeclibWrapperBundle": "" }
    },
    "target-dir": "DP/PHPSeclibWrapperBundle", 
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of ssh/sftp connections."
    }
}
